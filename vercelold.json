{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "rewrites": [
	{ 
      "source": "/api/trpc/products.list", 
      "destination": "/api/lista_produtos" 
    },
	{ "source": "/api/trpc/(.*)", "destination": "/api/index.js" },
	{ "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}