{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://vidabiz.butterfly-effect.dev/api/:path*"
    },
    {
      "source": "/style.css",
      "destination": "/style.css"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}